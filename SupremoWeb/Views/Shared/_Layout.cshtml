    <!--Toastr Logic-->
    <script>
        $(document).ready(function () {
        @if (TempData["MessageHeading"] != null && TempData["Message"] != null)
        {
            <text>
                var messageHeading = '@TempData["MessageHeading"]';
                var messageBody = '@TempData["Message"]';
                switch (messageHeading) {
                    case "OK":
                        toastr.success(messageBody);
                        break;
                    case "AVISO":
                        toastr.warning(messageBody);
                        break;
                    case "FALHA":
                        toastr.error(messageBody);
                        break;
                    default:
                        toastr.info(messageBody);
                        break;
                }
            </text>
        }
        });
    </script>

